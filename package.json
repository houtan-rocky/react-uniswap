{
  "name": "react-uniswap",
  "private": false,
  "version": "1.0.6",
  "type": "module",
  "description": "A clean Uniswap swap widget package for easy integration",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "npm run build",
    "start": "serve -s dist",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "keywords": [
    "uniswap",
    "swap",
    "widget",
    "defi",
    "react",
    "ethereum"
  ],
  "peerDependencies": {
    "@uniswap/sdk-core": ">=5.0.0",
    "@wagmi/core": ">=2.0.0",
    "wagmi": ">=2.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "viem": ">=2.0.0"
  },
  "dependencies": {
    "@reown/appkit": "^1.4.1",
    "@reown/appkit-adapter-wagmi": "^1.4.1",
    "@tanstack/react-query": "^5.60.5",
    "@uniswap/smart-order-router": "^4.21.17",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "@uniswap/v3-sdk": "^3.18.1",
    "ethers": "5.7.2",
    "jsbi": "^4.3.0",
    "react-icons": "^5.3.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "terser": "^5.41.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^3.7.3"
  }
}
